{
  "$schema": "https://developers.reddit.com/schema/config-file.v1.json",
  "name": "lootofolympus",
  "post": {
    "dir": "dist/client",
    "entrypoints": {
      "default": {
        "entry": "index.html"
      }
    }
  },
  "server": {
    "dir": "dist/server",
    "entry": "index.cjs"
  },
  "media": {},
  "menu": {
    "items": [
      {
        "label": "Create a new post",
        "description": "lootofolympus",
        "location": "subreddit",
        "forUserType": "moderator",
        "endpoint": "/internal/menu/post-create"
      }
    ]
  },
  "triggers": {
    "onAppInstall": "/internal/on-app-install"
  },
  "dev": {
    "subreddit": "lootofolympus_dev"
  },
  "scheduler": {
    "tasks": {
      "hourly-post-task": {
        "endpoint": "/internal/scheduler/hourly-post-task",
        "cron": "0 * * * *"
      }
    }
  }
}
